// simple nested

.m-home{
    display: block;
    > div, ul{
        border: 2px solid #ccc;
        a{
            color: #fff;
            &:hover{
               text-decoration: none; 
            }
            .m-home-name %{
                content: '.m-home-name in %';
            }
        }
    }
}



.btn{
    color: 10px;
}
.m-hoem{
    left:10px;
    p{
        a{
            @extend .btn;
        }
    }
}

.m-news{
    li{
        border-bottom: 1px dotted #ddd;
        @media (min-width: 1442px){
            width: 49%;
            float: left;
            &:nth-child(2n){
                border-left: 1px dashed #ddd;
            }
            
        }
    }
}

.ms-form{
    // 真不想重复写这么多啊
    input[type="text"],
    input[type="password"],
    input[type="email"],
    input[type="url"],
    input[type="date"],
    input[type="month"],
    input[type="time"],
    input[type="range"],
    select{
      display: inline-block;
      .ms-form-stack %{
        display: block;
      }
    }
}

